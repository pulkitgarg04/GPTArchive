{
    "manifest_version": 3,
    "name": "GPTArchive",
    "version": "1.0",
    "description": "Save and bookmark your ChatGPT chats for easy access.",
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "https://chat.openai.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "index.html"
    }
}